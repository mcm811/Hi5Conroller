<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:layout_gravity="center"
	android:background="@color/white"
	android:minHeight="0dp"
	android:minWidth="25dp">
	<!--android:background="@color/tab2_textview_background">-->
	<TextView
		android:id="@+id/tvTitleOutputData"
		android:layout_width="30dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="8dp"
		android:gravity="right|center_horizontal|end"
		android:hint="@string/OutputData"
		android:text="@string/OutputData"
		android:textSize="15sp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="@+id/tvTitleSqueezeForce"
		/>

	<TextView
		android:id="@+id/tvTitleOutputDataLabel"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="0dp"
		android:gravity="right|center_horizontal|end"
		android:hint="@string/output_data"
		android:text="@string/output_data"
		android:textSize="8sp"
		app:layout_constraintBottom_toBottomOf="@+id/tvTitleSqueezeForceLabel"
		app:layout_constraintStart_toStartOf="@+id/tvTitleOutputData"
		tools:ignore="SmallSp"/>

	<TextView
		android:id="@+id/tvTitleSqueezeForce"
		android:layout_width="60dp"
		android:layout_height="wrap_content"
		android:layout_marginBottom="0dp"
		android:layout_marginStart="48dp"
		android:layout_marginTop="2dp"
		android:gravity="right|center_horizontal|end"
		android:hint="@string/SqueezeForce"
		android:text="@string/SqueezeForce"
		android:textSize="15sp"
		app:layout_constraintBottom_toTopOf="@+id/tvTitleSqueezeForceLabel"
		app:layout_constraintStart_toEndOf="@+id/tvTitleOutputData"
		app:layout_constraintTop_toTopOf="parent"/>

	<TextView
		android:id="@+id/tvTitleSqueezeForceLabel"
		android:layout_width="60dp"
		android:layout_height="wrap_content"
		android:layout_marginBottom="2dp"
		android:gravity="right|center_horizontal|end"
		android:hint="@string/squeeze_force"
		android:text="@string/squeeze_force"
		android:textSize="8sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toStartOf="@+id/tvTitleSqueezeForce"
		tools:ignore="SmallSp"/>

	<TextView
		android:id="@+id/tvTitleOutputType"
		android:layout_width="30dp"
		android:layout_height="wrap_content"
		android:layout_marginEnd="16dp"
		android:layout_marginTop="0dp"
		android:gravity="right|center_horizontal|end"
		android:hint="@string/OutputType"
		android:text="@string/OutputType"
		android:textSize="15sp"
		app:layout_constraintEnd_toStartOf="@+id/tvTitleMoveTipClearance"
		app:layout_constraintHorizontal_bias="1.0"
		app:layout_constraintStart_toEndOf="@+id/tvTitleSqueezeForce"
		app:layout_constraintTop_toTopOf="@+id/tvTitleSqueezeForce"/>

	<TextView
		android:id="@+id/tvTitleOutputTypeLabel"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="0dp"
		android:gravity="right|center_horizontal|end"
		android:hint="@string/output_type"
		android:text="@string/output_type"
		android:textSize="8sp"
		app:layout_constraintBottom_toBottomOf="@+id/tvTitleSqueezeForceLabel"
		app:layout_constraintEnd_toEndOf="@+id/tvTitleOutputType"
		tools:ignore="SmallSp"/>

	<TextView
		android:id="@+id/tvTitleMoveTipClearance"
		android:layout_width="60dp"
		android:layout_height="wrap_content"
		android:layout_marginEnd="8dp"
		android:gravity="right|center_horizontal|end"
		android:hint="@string/MoveTipClearance"
		android:text="@string/MoveTipClearance"
		android:textSize="10sp"
		app:layout_constraintEnd_toStartOf="@+id/tvTitlePanelThickness"
		app:layout_constraintTop_toTopOf="@+id/tvTitleSqueezeForce"
		tools:ignore="SmallSp"/>

	<TextView
		android:id="@+id/tvTitleFixedTipClearance"
		android:layout_width="60dp"
		android:layout_height="wrap_content"
		android:layout_marginBottom="0dp"
		android:gravity="right|center_horizontal|end"
		android:hint="@string/FixedTipClearance"
		android:text="@string/FixedTipClearance"
		android:textSize="10sp"
		app:layout_constraintBottom_toBottomOf="@+id/tvTitleSqueezeForceLabel"
		app:layout_constraintEnd_toEndOf="@+id/tvTitleMoveTipClearance"
		app:layout_constraintStart_toStartOf="@+id/tvTitleMoveTipClearance"
		tools:ignore="SmallSp"/>

	<TextView
		android:id="@+id/tvTitlePanelThickness"
		android:layout_width="60dp"
		android:layout_height="wrap_content"
		android:layout_marginEnd="16dp"
		android:gravity="right|center_horizontal|end"
		android:hint="@string/PanelThickness"
		android:text="@string/PanelThickness"
		android:textSize="10sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toTopOf="@+id/tvTitleSqueezeForce"
		tools:ignore="SmallSp"
		/>

	<TextView
		android:id="@+id/tvTitleCommandOffset"
		android:layout_width="60dp"
		android:layout_height="wrap_content"
		android:layout_marginBottom="0dp"
		android:gravity="right|center_horizontal|end"
		android:hint="@string/CommandOffset"
		android:text="@string/CommandOffset"
		android:textSize="10sp"
		app:layout_constraintBottom_toBottomOf="@+id/tvTitleSqueezeForceLabel"
		app:layout_constraintEnd_toEndOf="@+id/tvTitlePanelThickness"
		app:layout_constraintStart_toStartOf="@+id/tvTitlePanelThickness"
		tools:ignore="SmallSp"/>

</android.support.constraint.ConstraintLayout>